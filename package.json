{
  "name": "cashflow-finder",
  "version": "1.0.0",
  "description": "Business listing platform for investors and brokers",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "ts-node-dev --respawn --transpile-only src/server/server.ts",
    "client:dev": "next dev -p 3001",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "next build",
    "start": "node dist/server.js",
    "start:production": "NODE_ENV=production node dist/server/server.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:production": "npx prisma migrate deploy",
    "db:seed": "node dist/server/scripts/populate-business-data.js",
    "import:all": "node dist/server/scripts/import-multi-source.js",
    "import:additional": "node dist/server/scripts/import-additional-sources.js",
    "setup-premium-user": "node dist/server/scripts/setup-premium-user.js",
    "deploy:production": "./deployment/production-deploy.sh",
    "docker:build": "docker build -t cashflow-finder .",
    "docker:run": "docker run -p 3000:3000 cashflow-finder",
    "security:audit": "npm audit --audit-level high",
    "clean": "rm -rf dist .next node_modules/.cache"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "@sendgrid/mail": "^8.1.5",
    "@stripe/stripe-js": "^2.1.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.0.0",
    "zod": "^3.22.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}